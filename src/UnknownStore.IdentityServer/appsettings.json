{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "CurrentDirectory": null, //initialize for startup
  "EmailHtmlFormMessage": {
    "PathForm": "UnknownStore.IdentityServer\\Common\\HTML\\EmailConfirmation.html",
    "ReplaceConfirm": "COFIRMEMAIL",
    "ReplaceDecline": "DECLINEEMAIL"
  },

  "SeedIdentity": {
    "Roles": "UnknownStore.DAL\\Data\\InitData\\Roles.json"
  },

  "Cookie": {
    "LoginPath": "/Auth/Login",
    "LogoutPath": "/Auth/Logout",
    "Name": "IdentityServer.Cookie"
  } 
}